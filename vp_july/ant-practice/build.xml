</target>

<target name="loans" depends="savings">
<echo message="welcome to loans"/>
<property name="patch" value="20" />
<echo message="patch value is : ${patch}" />
</target>

<target name="creditcard" >
<echo message="welcome to creditcard"/>
</target>

<echo message="welcome to banking end" />

</project>
